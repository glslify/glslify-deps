{
  "name": "glslify-deps",
  "version": "1.3.1",
  "engines": {
    "node": ">=4"
  },
  "scripts": {
    "test": "node test | tap-spec",
    "prepare": "npm run build",
    "build": "babel . --ignore node_modules --ignore lib --ignore test --out-dir lib",
    "coverage": "istanbul cover test/index.js && http-server coverage/lcov-report/glslify-deps"
  },
  "dependencies": {
    "@choojs/findup": "^0.2.1",
    "events": "^3.2.0",
    "glsl-resolve": "0.0.1",
    "glsl-tokenizer": "^2.1.5",
    "graceful-fs": "^4.2.4",
    "inherits": "^2.0.4",
    "map-limit": "0.0.1",
    "resolve": "^1.19.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.12.3",
    "@babel/preset-env": "^7.12.1",
    "glsl-random": "0.0.5",
    "glslify-hex": "^2.1.1",
    "husky": "^4.3.0",
    "istanbul": "^0.4.5",
    "standard": "*",
    "tap-spec": "^5.0.0",
    "tape": "^5.0.1"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "standard && npm test",
      "pre-push": "standard && npm test"
    }
  },
  "description": "Walk the dependency graph of a glslify shader.",
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stackgl/glslify-deps.git"
  },
  "author": "Hugh Kennedy <hughskennedy@gmail.com> (http://hughsk.io/)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/stackgl/glslify-deps/issues"
  },
  "homepage": "https://github.com/stackgl/glslify-deps#readme"
}
